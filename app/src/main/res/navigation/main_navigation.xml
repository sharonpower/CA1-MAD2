<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/reviewListFragment">

    <fragment
        android:id="@+id/reviewListFragment"
        android:name="ie.wit.perfectpizza.fragments.ReviewListFragment"
        android:label="fragment_review_list"
        tools:layout="@layout/fragment_review_list" >
        <action
            android:id="@+id/action_reviewListFragment_to_reviewFragment3"
            app:destination="@id/reviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/reviewFragment"
        android:name="ie.wit.perfectpizza.fragments.ReviewFragment"
        android:label="fragment_review"
        tools:layout="@layout/fragment_review" >
        <action
            android:id="@+id/action_reviewFragment_to_reviewListFragment"
            app:destination="@id/reviewListFragment" />
    </fragment>
</navigation>